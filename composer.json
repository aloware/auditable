{
    "name": "aloware/auditable",
    "description": "Enable auditing of Eloquent models's attributes, relations and custom properties",
    "type": "library",
    "require": {
        "php": "^8.1 || ^8.2",
        "laravel/framework": "^9 || ^10.0 || ^11.0"
    },
    "license": "Proprietary",
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "Aloware\\Auditable\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Diego Barreiro",
            "email": "diego@aloware.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Aloware\\Auditable\\Providers\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
